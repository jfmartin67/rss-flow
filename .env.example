# Vercel KV (Redis) credentials
# Get these from your Vercel dashboard: https://vercel.com/dashboard/stores
KV_URL=your_kv_url_here
KV_REST_API_URL=your_kv_rest_api_url_here
KV_REST_API_TOKEN=your_kv_rest_api_token_here
KV_REST_API_READ_ONLY_TOKEN=your_kv_rest_api_read_only_token_here

# KV Key Prefix (namespace) - Important when sharing KV instance with other projects
# This prevents key collisions between different applications
# Default: "rss-flow" (optional, can be customized)
KV_PREFIX=rss-flow
